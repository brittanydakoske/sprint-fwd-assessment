<div>
  <p>
    <span style="font-weight: bolder;">Team</span>
    <span><%= @member.team.name %></span>
  </p>
  <p>
    <span style="font-weight: bolder;">Id</span>
    <span><%= @member.id %></span>
  </p>
  <p>
    <span style="font-weight: bolder;">First Name</span>
    <span><%= @member.first_name %></span>
  </p>
  <p>
    <span style="font-weight: bolder;">Last Name</span>
    <span><%= @member.last_name %></span>
  </p>
  <p>
    <span style="font-weight: bolder;">City</span>
    <span><%= @member.city %></span>
  </p>
  <p>
    <span style="font-weight: bolder;">State</span>
    <span><%= @member.state %></span>
  </p>
  <p>
    <span style="font-weight: bolder;">Country</span>
    <span><%= @member.country %></span>
  </p>
  <p>
    <span style="font-weight: bolder;">Projects</span>
    <% if @member.projects.empty? %>
      <span>None</span>
    <% else %>
      <% @member.projects.each do |project| %>
        <span><%= project.name %>, </span>
      <% end %>
    <% end %>
  </p>
  <%= link_to 'Back', api_members_path %>
</div>