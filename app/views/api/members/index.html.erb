<div>
  <h3>Members</h3>
  <table class="teams" style="width: 100%">
    <tr>
      <th>Id</th>
      <th>Team</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>City</th>
      <th>State</th>
      <th>Country</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
    <% @members.each do |member| %>
      <tr class="team">
        <td><%= member.id %></td>
        <td><%= member.team.name %></td>
        <td><%= member.first_name %></td>
        <td><%= member.last_name %></td>
        <td><%= member.city %></td>
        <td><%= member.state %></td>
        <td><%= member.country %></td>
        <td>
          <%= link_to 'Details', api_member_path(member) %>
        </td>
        <td>
          <%= link_to 'Edit', edit_api_member_path(member) %>
        </td>
        <td>
          <%= button_to 'Delete', api_member_path(member), method: :delete %>
        </td>
      </tr>
    <% end %>
  </table>
  <hr />
  <%= link_to "New Member", new_api_member_path %>
</div>

<style>
    td,
    th {
        text-align: left;
    }
</style>